{
  "name": "@yesoreyeram/nearley",
  "version": "3.0.0-beta.2",
  "description": "Simple, fast, powerful parser toolkit for JavaScript.",
  "main": "lib/nearley.js",
  "files": [
    "bin/",
    "lib/",
    "builtin/"
  ],
  "bin": {
    "nearleyc": "bin/nearleyc.js",
    "nearley-test": "bin/nearley-test.js",
    "nearley-unparse": "bin/nearley-unparse.js"
  },
  "keywords": [
    "parser",
    "parse",
    "generator",
    "compiler",
    "compile",
    "grammar",
    "language"
  ],
  "scripts": {
    "bootstrap": "mocha test/bootstrap.test.js && bin/nearleyc.js lib/nearley-language-bootstrapped.ne > tmp && mv tmp lib/nearley-language-bootstrapped.js && echo bootstrapped ok",
    "benchmark": "benchr test/benchmark.js",
    "test": "mocha test/*.test.js",
    "profile": "bin/nearleyc.js test/grammars/parens.ne > test/grammars/parens.js && node test/profile.js"
  },
  "author": "Sriramajeyam Sugumaran",
  "contributors": [
    {
      "name": "Original Contributors",
      "url": "https://github.com/kach/nearley/graphs/contributors"
    },
    {
      "name": "Contributors",
      "url": "https://github.com/yesoreyeram/nearley/graphs/contributors"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yesoreyeram/nearley.git"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "moo": "^0.5.0",
    "randexp": "0.4.6"
  },
  "devDependencies": {
    "@types/moo": "^0.3.0",
    "@types/node": "^7.0.27",
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "benchr": "^3.2.0",
    "expect": "^1.20.2",
    "microtime": "^3.1.1",
    "mocha": "^8.2.1",
    "typescript": "^2.6.1"
  }
}
